{
  "name": "@se-oss/md5",
  "version": "0.0.0",
  "description": "A simple, zero-dependency MD5 hashing library for Node.js and browsers. Automatically uses native crypto in Node.js for performance.",
  "keywords": [
    "md5",
    "hash",
    "crypto",
    "typescript",
    "nodejs"
  ],
  "homepage": "https://github.com/shahradelahi/ts-md5",
  "repository": "github:shahradelahi/ts-md5",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "browser": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "browser": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "!**/*.d.cts"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "pnpm typecheck && tsup",
    "clean": "git clean -dfx node_modules dist .tsbuildinfo",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm build && pnpm lint && pnpm format:check && pnpm test",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@se-oss/delay": "^1.0.0",
    "@shahrad/eslint-config": "^1.0.1",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.2.0",
    "@types/md5": "^2.3.6",
    "@types/node": "^24.7.1",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "md5": "^2.3.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
