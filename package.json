{
  "name": "@se-oss/md5",
  "version": "1.1.0",
  "description": "A simple, zero-dependency MD5 hashing library for Node.js and browsers. Automatically uses native crypto in Node.js for performance.",
  "keywords": [
    "md5",
    "hash",
    "crypto",
    "typescript",
    "nodejs"
  ],
  "homepage": "https://github.com/shahradelahi/ts-md5",
  "repository": "github:shahradelahi/ts-md5",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node.js",
        "default": "./dist/node.cjs"
      },
      "default": {
        "import": "./dist/browser.js",
        "default": "./dist/browser.cjs"
      }
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts",
      "default": "./dist/node.cjs"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.cjs"
    }
  },
  "main": "dist/node.cjs",
  "browser": "dist/browser.js",
  "types": "dist/node.d.ts",
  "files": [
    "dist/**",
    "!**/*.d.cts"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "pnpm typecheck && tsup",
    "ci:publish": "changeset publish",
    "clean": "git clean -dfx \"**/node_modules/**\" dist .tsbuildinfo",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm build && pnpm lint && pnpm format:check && pnpm test",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@edge-runtime/vm": "^5.0.0",
    "@se-oss/delay": "^1.0.0",
    "@shahrad/eslint-config": "^1.0.1",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.2.0",
    "@types/node": "^25.2.2",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
